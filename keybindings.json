// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+i",
        "command": "composerMode.agent"
    },
    {
        "key": "down",
        "command": "editor.action.nextMatchFindAction",
        "when": "editorFocus && (findInputFocussed || replaceInputFocussed)"
    },
    {
        "key": "enter",
        "command": "-editor.action.nextMatchFindAction",
        "when": "editorFocus && findInputFocussed"
    },
    {
        "key": "up",
        "command": "editor.action.previousMatchFindAction",
        "when": "editorFocus && (findInputFocussed || replaceInputFocussed)"
    },
    {
        "key": "shift+enter",
        "command": "-editor.action.previousMatchFindAction",
        "when": "editorFocus && findInputFocussed"
    },
    {
        "key": "ctrl+,",
        "command": "git.merge"
    },
    {
        "key": "ctrl+shift+,",
        "command": "git.mergeAbort"
    },
    {
        "key": "ctrl+/",
        "command": "git.checkout"
    },
    {
        "key": "ctrl+alt+/",
        "command": "gitBranchPrefill.createBranchFromCurrent"
    },
    {
        "key": "cmd+shift+k",
        "command": "git.push"
    },
    {
        "key": "ctrl+]",
        "command": "editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+[",
        "command": "editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+s",
        "command": "editor.action.transformToSnakecase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+c",
        "command": "editor.action.transformToCamelcase",
        "when": "editor.hasSelection && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+p",
        "command": "editor.action.transformToPascalcase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+u",
        "command": "editor.action.transformToUppercase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+k",
        "command": "editor.action.transformToKebabcase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+l",
        "command": "editor.action.transformToLowercase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+alt+t",
        "command": "editor.action.transformToTitlecase",
        "when": "editorTextFocus && editor.hasSelection && !editorReadonly"
    },
    {
        "key": "shift+enter",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "\\\r\n"
        },
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+/",
        "command": "editor.action.fontZoomOut",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+,",
        "command": "-editor.action.fontZoomOut",
        "when": "editorTextFocus"
    }
]