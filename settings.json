{
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "editor.fontSize": 20,
    "editor.fontFamily": "\"Jetbrains Mono\"",
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontFamily": "\"Jetbrains Mono\"",
    "editor.renderWhitespace": "all",
    "files.autoSave": "onFocusChange",
    "terminal.integrated.fontLigatures.enabled": true,
    "editor.inlayHints.padding": true,
    "editor.guides.bracketPairs": "active",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "jetbrains-file-icon-theme-dark",
    "workbench.sideBar.location": "right",
    "files.associations": {
        "*.njk": "twig",
        "*.js": "javascript"
    },
    "cursor.composer.textSizeScale": 1.15,
    "cursor.terminal.usePreviewBox": true,
    "cursor.cpp.enablePartialAccepts": true,
    "editor.lineNumbers": "relative",
    "editor.cursorStyle": "line-thin",
    "editor.fastScrollSensitivity": 10,
    "editor.formatOnPaste": true,
    "editor.minimap.scale": 2,
    "files.autoSaveWorkspaceFilesOnly": true,
    "workbench.list.smoothScrolling": true,
    "reactSnippets.settings.importReactOnTop": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "search.defaultViewMode": "tree",
    "editor.rename.enablePreview": false,
    "workbench.editor.enablePreview": false,
    "workbench.productIconTheme": "jetbrains-product-icon-theme",
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "editor.minimap.enabled": false,
    "explorer.compactFolders": false,
    "explorer.autoReveal": false,
    "path-intellisense.extensionOnImport": true,
    "explorer.fileNesting.expand": false,
    "search.useParentIgnoreFiles": true,
    "scm.defaultViewMode": "tree",
    "git.checkoutType": [
        "remote",
        "local"
    ],
    "git.branchSortOrder": "alphabetically",
    "git.blame.editorDecoration.enabled": true,
    "git.branchPrefix": "wfm/",
    "editor.trimAutoWhitespace": true,
    "editor.trimTrailingWhitespace": false,
    "files.trimTrailingWhitespaceInRegexAndStrings": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "cursor.composer.shouldAllowCustomModes": true,
    "git.branchProtection": [
        "wfm/dev"
    ],
    "git.closeDiffOnOperation": true,
    "git.fetchOnPull": true,
    "git.mergeEditor": true,
    "git.pullBeforeCheckout": true,
    "git.terminalGitEditor": true,
    "git.autoStash": true,
    "github.gitProtocol": "ssh",
    "git.branchValidationRegex": "(doudar\\/)?(wfm|fe|ta|analytics|ui|qa|dev|sync)\\/(feat|hotfix|eng|fix|merge|automation|docs|local)\\/[a-z\\-]+",
    "git.useCommitInputAsStashMessage": true,
    "git.pruneOnFetch": true,
    "[javascript]": {
        "editor.suggest.showConstants": true,
        "editor.maxTokenizationLineLength": 2500,
    },
    "eslint-service.alwaysShowRuleFailuresAsWarnings": true,
    "tslint.jsEnable": true,
    "workbench.activityBar.orientation": "vertical",
}
